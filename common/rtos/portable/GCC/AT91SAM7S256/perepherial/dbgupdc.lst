 405:./rtos/portable/GCC/AT91SAM7S256/lib_AT91SAM7S256.h **** 		return 0;
 406:./rtos/portable/GCC/AT91SAM7S256/lib_AT91SAM7S256.h **** 	}
 407:./rtos/portable/GCC/AT91SAM7S256/lib_AT91SAM7S256.h **** }
