1384:./rtos/portable/GCC/AT91SAM7S256/lib_AT91SAM7S256.h **** 	pRTTC->RTTC_RTMR &= ~0xFFFF;
1385:./rtos/portable/GCC/AT91SAM7S256/lib_AT91SAM7S256.h **** 	pRTTC->RTTC_RTMR |= (rtpres & 0xFFFF);
