2488:./rtos/portable/GCC/AT91SAM7S256/lib_AT91SAM7S256.h **** 		baud_value /= 10;
2489:./rtos/portable/GCC/AT91SAM7S256/lib_AT91SAM7S256.h **** 	return baud_value;
2490:./rtos/portable/GCC/AT91SAM7S256/lib_AT91SAM7S256.h **** }
