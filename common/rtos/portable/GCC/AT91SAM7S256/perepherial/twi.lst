1178:./rtos/portable/GCC/AT91SAM7S256/lib_AT91SAM7S256.h **** 		case AT91C_PMC_CSS_SLOW_CLK: // Slow clock selected
1179:./rtos/portable/GCC/AT91SAM7S256/lib_AT91SAM7S256.h **** 			return slowClock / prescaler;
